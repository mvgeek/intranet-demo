//
// Development Configuration
// This file contains development-specific build settings
//

// Code Signing Settings for Development
CODE_SIGN_IDENTITY = iPhone Developer
CODE_SIGN_STYLE = Automatic
DEVELOPMENT_TEAM = YOUR_DEVELOPMENT_TEAM_ID

// Bundle Identifier
PRODUCT_BUNDLE_IDENTIFIER = com.vilnius.intranet.dev

// Provisioning Profile
PROVISIONING_PROFILE_SPECIFIER = 

// App Transport Security - Allow HTTP for development
OTHER_PLIST_FLAGS = -DDEVELOPMENT=1

// Debug Settings
SWIFT_ACTIVE_COMPILATION_CONDITIONS = DEBUG $(inherited)
GCC_PREPROCESSOR_DEFINITIONS = DEBUG=1 $(inherited)

// Deployment Target
IPHONEOS_DEPLOYMENT_TARGET = 15.0

// Architecture
ARCHS = $(ARCHS_STANDARD)
VALID_ARCHS = arm64 arm64e x86_64

// Swift Settings
SWIFT_VERSION = 5.0
SWIFT_OPTIMIZATION_LEVEL = -Onone

// Bitcode
ENABLE_BITCODE = NO

// App Icon
ASSETCATALOG_COMPILER_APPICON_NAME = AppIcon-Dev